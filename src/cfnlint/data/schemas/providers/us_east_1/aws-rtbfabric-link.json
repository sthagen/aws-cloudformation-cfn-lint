     "maxItems": 2,
     "minItems": 1,
     "type": "array",
     "uniqueItems": true
    },
    "ResponseLoggingPercentage": {
     "maximum": 100,
     "minimum": 0,
     "type": "number"
    }
   },
   "required": [
    "HttpCode",
    "Action",
    "LoggingTypes"
   ],
   "type": "object"
  },
  "ResponderErrorMaskingLoggingType": {
   "enum": [
    "NONE",
    "METRIC",
    "RESPONSE"
   ],
   "type": "string"
  },
