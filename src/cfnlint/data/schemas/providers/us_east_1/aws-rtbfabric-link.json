      "LinkApplicationLogSampling": {
       "additionalProperties": false,
       "properties": {
        "ErrorLog": {
         "maximum": 100.0,
         "minimum": 0.0,
         "type": "number"
        },
        "FilterLog": {
         "maximum": 100.0,
         "minimum": 0.0,
         "type": "number"
        }
       },
       "required": [
        "ErrorLog",
        "FilterLog"
       ],
       "type": "object"
      }
     },
     "required": [
      "LinkApplicationLogSampling"
     ],
     "type": "object"
    }
   },
   "required": [
    "ApplicationLogs"
   ],
   "type": "object"
  },
  "LinkStatus": {
