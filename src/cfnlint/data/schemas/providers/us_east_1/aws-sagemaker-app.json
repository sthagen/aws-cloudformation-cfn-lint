   "type": "string"
  },
  "AppName": {
   "maxLength": 63,
   "minLength": 1,
   "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}",
   "type": "string"
  },
  "AppType": {
   "enum": [
    "JupyterServer",
    "KernelGateway",
    "RStudioServerPro",
    "RSessionGateway",
    "Canvas"
   ],
   "type": "string"
  },
  "BuiltInLifecycleConfigArn": {
   "maxLength": 256,
